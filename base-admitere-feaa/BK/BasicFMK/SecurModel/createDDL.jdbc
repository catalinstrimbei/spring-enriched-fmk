CREATE TABLE LINIEFACTURA (ID INTEGER NOT NULL, CANTITATE FLOAT, PRETUNIT FLOAT, LINIE INTEGER, FACTURA_NRFACT INTEGER, PRODUS_CODPR INTEGER, PRIMARY KEY (ID))
CREATE TABLE CLIENT (CODCL INTEGER NOT NULL, DENCL VARCHAR(255), TELEFON VARCHAR(255), CODFISCAL VARCHAR(255), CODPOST VARCHAR(255), STRADA VARCHAR(255), NR VARCHAR(255), PRIMARY KEY (CODCL))
CREATE TABLE FACTURA (NRFACT INTEGER NOT NULL, OBS VARCHAR(255), DATAFACT DATE, CLIENT_CODCL INTEGER, PRIMARY KEY (NRFACT))
CREATE TABLE PRODUS (CODPR INTEGER NOT NULL, DENPR VARCHAR(255), PROCTVA FLOAT, GRUPA VARCHAR(255), INDICATIV VARCHAR(255), DENUMIRE VARCHAR(255), PRIMARY KEY (CODPR))
ALTER TABLE LINIEFACTURA ADD CONSTRAINT LNFCTURAFCTRNRFACT FOREIGN KEY (FACTURA_NRFACT) REFERENCES FACTURA (NRFACT)
ALTER TABLE LINIEFACTURA ADD CONSTRAINT LNFACTURAPRDSCODPR FOREIGN KEY (PRODUS_CODPR) REFERENCES PRODUS (CODPR)
ALTER TABLE FACTURA ADD CONSTRAINT FACTURACLIENTCODCL FOREIGN KEY (CLIENT_CODCL) REFERENCES CLIENT (CODCL)
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL, PRIMARY KEY (SEQ_NAME))
