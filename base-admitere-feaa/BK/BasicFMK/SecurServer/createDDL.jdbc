CREATE TABLE CLIENT (CODCL INTEGER NOT NULL, DENCL VARCHAR(255), TELEFON VARCHAR(255), CODFISCAL VARCHAR(255), CODPOST VARCHAR(255), STRADA VARCHAR(255), NR VARCHAR(255), PRIMARY KEY (CODCL))
CREATE TABLE PRODUS (CODPR INTEGER NOT NULL, UM VARCHAR(255), GRUPA VARCHAR(255), DENPR VARCHAR(255), PROCTVA FLOAT, PRIMARY KEY (CODPR))
